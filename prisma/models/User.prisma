model User {
  id           String   @id @default(cuid())
  reference_id String   @unique
  role_id      String
  name         String?
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt

  role         Role          @relation(fields: [role_id], references: [id])
  events       Event[]
  integrations Integration[]

  @@map("Users")
}
